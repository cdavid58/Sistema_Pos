<Window x:Class="WpfApp1.forms.Modules.Facturacion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1.forms.Modules"
        mc:Ignorable="d"
        Title="Facturacion" Height="450" Width="800" WindowState="Maximized">
    <Grid>
        <TabControl Name="tabControl" SelectionChanged="TabControl_SelectionChanged">
            <TabItem Header="Nueva Pestaña">
                <WebBrowser Name="webBrowser"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
